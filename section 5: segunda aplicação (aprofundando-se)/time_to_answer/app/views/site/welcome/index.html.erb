<h1>Site::Welcome#index</h1>
<p>Find me in app/views/site/welcome/index.html.erb</p>


<% if user_signed_in? %>
  <p>Usuário: <span style="color: green;">LOGADO</span></p>
  <p><%= current_user.email %></p>
<% else %>
  <p>Usuário: <span style="color: red;">NÃO LOGADO</span></p>
<% end %>


<% if admin_signed_in? %>
  <p>Administrador: <span style="color: green;">LOGADO</span></p>
  <p><%= current_admin.email %></p>
<% else %>
  <p>Administrador: <span style="color: red;">NÃO LOGADO</span></p>
<% end %>
